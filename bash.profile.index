ignoreFiles=("bash.profile.index" "README.md" "newEnvScripts" ".gitignore" "synthwave84.css")

array_contains () {
    local seeking=$1; shift
    local in=1
    for element; do
        if [[ $element == $seeking ]]; then
            in=0
            break
        fi
    done
    return $in
}

for entry in $(ls ${HOME}/.Dotfiles/);
do
  array_contains "$entry" "${ignoreFiles[@]}" && : || source ${HOME}/.Dotfiles/$entry
done
